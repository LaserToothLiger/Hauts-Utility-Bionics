<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Alpha Genes</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				<li Class="PatchOperationFindMod">
					<mods>
						<li>Anomaly</li>
					</mods>
					<match Class="PatchOperationAdd">
						<xpath>/Defs/AlphaGenes.BlackListedTraitsDef[defName="AG_BlackListedTraits"]/blackListedTraits</xpath>
						<value>
							<li>HVB_HomunculusTrait</li>
						</value>
					</match>
				</li>
			</operations>
		</match>
	</Operation>
</Patch>