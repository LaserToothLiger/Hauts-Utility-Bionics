      <HediffDef>HVB_MediAnalyzer</HediffDef>
    </descriptionHyperlinks>
    <jobString>Installing medi-analyzer.</jobString>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>HVB_MediAnalyzer</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
