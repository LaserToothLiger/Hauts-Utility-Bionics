        <li>HVB_HostileEnvironmentSkin</li>
      </thingDefs>
    </fixedIngredientFilter>
    <appliedOnFixedBodyParts>
      <li>Torso</li>
    </appliedOnFixedBodyParts>
    <addsHediff>HVB_HostileEnvironmentSkin</addsHediff>
    <incompatibleWithHediffTags>
